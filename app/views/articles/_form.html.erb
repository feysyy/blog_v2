<%= form_with model: article do |form| %>
  <div>
    <%= form.label :name %>
    <br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :body %>
    <br>
    <%= form.text_field :body %>
  </div>

  <div>
    <%= form.submit %>
  </div>

<% end %>

<%= link_to "Home", articles_path %>  